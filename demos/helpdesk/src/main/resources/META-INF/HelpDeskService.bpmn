    <userTask id="_10" name="Review Ticket" >
      <extensionElements>
        <tns:onExit-script scriptFormat="http://www.java.com/java">
          <script>Ticket ticket = (Ticket)kcontext.getVariable("ticket");
if (ticket == null) {
    ticket = new Ticket();
}
String status;
switch (new Random().nextInt(3)) {
    case 0: status = "approved"; break;
    case 1: status = "rejected"; break;
    default: status = "requested";
}
ticket.setStatus(status);
kcontext.setVariable("ticket", ticket);</script>
        </tns:onExit-script>
        <tns:onExit-script>
          <script></script>
        </tns:onExit-script>
      </extensionElements>
