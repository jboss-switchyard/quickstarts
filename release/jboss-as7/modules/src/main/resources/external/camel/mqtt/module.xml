<module xmlns="urn:jboss:module:1.1" name="org.apache.camel.mqtt">
    <resources>
        <resource-root path="camel-mqtt-${version.org.apache.camel}.jar" />
        <resource-root path="mqtt-client-${version.org.fusesource.mqtt-client}.jar" />
        <resource-root path="hawtbuf-${version.org.fusesource.hawtbuf}.jar" />
        <resource-root path="hawtdispatch-${version.org.fusesource.hawtdispatch}.jar" />
        <resource-root path="hawtdispatch-transport-${version.org.fusesource.hawtdispatch}.jar" />
    </resources>
    <dependencies>
        <module name="javax.api" />
        <module name="org.apache.camel.core" />
        <module name="asm.asm-tree" optional="true" />
        <module name="org.slf4j.jcl-over-slf4j" optional="true" />
    </dependencies>
</module>
